{
    "compilerOptions": {
        "target": "es2017",
        "allowSyntheticDefaultImports": false,
        "baseUrl": "./",
        "paths": {
            "src/*": [
                "src/*"
            ],
            "components/*": [
                "src/Components/*"
            ],
            "store/*": [
                "src/Store/*"
            ],
            "assets/*": [
                "assets/*"
            ],
            "pages/*": [
                "src/Pages/*"
            ],
        }
    },
    "exclude": [
        "node_modules",
        "dist"
    ]
}